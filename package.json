    "@testing-library/react": "^12.1.2",
    "@types/apollo-upload-client": "^17.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/chai": "^4.3.0",
    "@types/ejs": "^3.1.0",
    "@types/express": "^4.17.13",
    "@types/graphql-upload": "^8.0.10",
    "@types/jest": "^27.4.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.8",
    "@types/pg": "^8.6.4",
    "@types/react": "^17.0.32",
    "@types/react-datepicker": "^4.3.4",
