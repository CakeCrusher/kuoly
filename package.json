{
  "name": "givespace-site",
  "version": "1.0.0",
  "main": "./server/index.ts",
  "license": "MIT",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "concurrently -k \" yarn dev-client \" \" yarn dev-server \"",
    "dev-server": "nodemon server/index.ts",
    "dev-client": "webpack serve --config webpack.config.js",
    "build": "yarn build-server && yarn build-client",
    "build-server": "yarn tsc && cp -r server/views dist/ && cp -r server/gql/typedefs dist/gql/typedefs",
    "build-client": "webpack --config webpack.config.js"
  },
  "dependencies": {
    "@apollo/client": "^3.5.7",
    "@babel/core": "^7.15.8",
    "@babel/node": "^7.16.8",
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/runtime": "^7.16.0",
    "@google-cloud/storage": "^5.18.0",
    "@graphql-tools/schema": "^8.3.1",
    "@types/graphql": "^14.5.0",
    "apollo-server-express": "^3.6.1",
    "apollo-upload-client": "^17.0.0",
    "babel-loader": "^8.2.3",
    "body-parser": "^1.19.1",
    "bootstrap": "^5.1.3",
    "cheerio": "^1.0.0-rc.10",
    "css-loader": "^6.4.0",
    "dotenv": "^11.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.2",
    "global": "^4.4.0",
    "graphql": "^16.1.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-upload": "^13.0.0",
    "html-webpack-plugin": "^5.5.0",
    "interpolate-html-plugin": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "nodemon": "^2.0.15",
    "pg": "^8.7.1",
    "react": "^17.0.2",
    "react-datepicker": "^4.6.0",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "style-loader": "^3.3.1",
    "subscriptions-transport-ws": "^0.11.0",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4",
    "uuid": "^8.3.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
